{
  "version": 2,
  "buildCommand": "cd my-project && npm run build",
  "outputDirectory": "my-project/dist",
  "installCommand": "cd my-project && npm install",
  "devCommand": "cd my-project && npm run dev",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
